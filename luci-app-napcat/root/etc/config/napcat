
config main
	option image 'docker.cnb.cool/3wlh/docker-sync/mlikiowa-napcat-docker:latest'
	option container_name 'napcat'
	option enable '1'
	option main_port '6099'
	option api_port '3000'
	option ws_port '3001'
	option network_mode 'bridge'
	option autostart '1'

config directory
	option data_dir '/etc/napcat/QQ'
	option config_dir '/etc/napcat/config'

config advanced

