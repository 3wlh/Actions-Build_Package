{
	"luci-app-photopea": {
		"description": "Grant UCI access for luci-app-photopea",
		"read": {
			"uci": [ "photopea" ],
			"ubus": {
				"service": [ "list" ]
			},
			"file": {
				"/usr/sbin/photopea": [ "exec" ],
				"/*": [ "read" ]
			}
		},
		"write": {
			"file": {
				"/*": [ "write" ]
			}
		}
	}
}